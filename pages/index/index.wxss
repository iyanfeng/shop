.home-swiper{
    width: 750rpx;
    height: 340rpx;
}
.h-swiper-image{
 width: 100%;
}

.nav{
    display: flex;
}
.n-navigator{
    flex:1;
    padding: 10rpx;
}
.n-navigator-image{
    width: 100%;
}

/* 楼层 */
.f-item-navgators{
    overflow: hidden;
    padding: 10rpx;
}
.f-item-navgators{
    margin-bottom: 10rpx;
}
.fi-navgator{
    float: left;
    width: 33.33%;
}
.fi-navgator:nth-last-child(-n+4){
    /* 原图宽高：232*386   保持宽高比缩放232/386 = 33.33vw/h =>h =33.33vw*386/232 */
    height: 27.72vw;
    border-left: 10rpx solid #fff;
}
.fi-navgator:nth-child(2),
.fi-navgator:nth-child(3){
    border-bottom: 10rpx solid #fff;
}
.fi-image{
    width: 100%;
    height: 100%;
}